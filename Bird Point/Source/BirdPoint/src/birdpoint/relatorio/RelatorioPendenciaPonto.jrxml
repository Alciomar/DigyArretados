	<queryString>
		<![CDATA[SELECT
     ponto.`idPonto` AS ponto_idPonto,
     ponto.`atestadoPonto` AS ponto_atestadoPonto,
     ponto.`dataPonto` AS ponto_dataPonto,
     ponto.`diaDaSemana` AS ponto_diaDaSemana,
     ponto.`emailPonto` AS ponto_emailPonto,
     ponto.`horaEntradaPonto` AS ponto_horaEntradaPonto,
     ponto.`horaSaidaPonto` AS ponto_horaSaidaPonto,
     ponto.`justEntrada` AS ponto_justEntrada,
     ponto.`justSaida` AS ponto_justSaida,
     ponto.`turnoPonto` AS ponto_turnoPonto,
     ponto.`anoExercicio_idAnoExercicio` AS ponto_anoExercicio_idAnoExercicio,
     ponto.`professor_idProfessor` AS ponto_professor_idProfessor,
     ponto.`justificativaPonto` AS ponto_justificativaPonto,
     professor.`idProfessor` AS professor_idProfessor,
     professor.`RGProfessor` AS professor_RGProfessor,
     professor.`bairroProfessor` AS professor_bairroProfessor,
     professor.`cidadeProfessor` AS professor_cidadeProfessor,
     professor.`cpfProfessor` AS professor_cpfProfessor,
     professor.`digitalDireita` AS professor_digitalDireita,
     professor.`digitalEsquerda` AS professor_digitalEsquerda,
     professor.`emailProfessor` AS professor_emailProfessor,
     professor.`fotoProf` AS professor_fotoProf,
     professor.`nomeProfessor` AS professor_nomeProfessor,
     professor.`numeroCasa` AS professor_numeroCasa,
     professor.`ruaProfessor` AS professor_ruaProfessor,
     professor.`situacaoProfessor` AS professor_situacaoProfessor,
     professor.`telefoneProfessor` AS professor_telefoneProfessor,
     professor.`titulacaoProfessor` AS professor_titulacaoProfessor,
     professor.`receberEmail` AS professor_receberEmail
FROM
     `professor` professor INNER JOIN `ponto` ponto ON professor.`idProfessor` = ponto.`professor_idProfessor`]]>
	</queryString>
